config ADK_TARGET_WITH_MMU
	bool "MMU"
	depends on !ADK_APPLIANCE_TOOLCHAIN && !ADK_TARGET_CHOOSE_ARCH
	depends on !ADK_TARGET_ARCH_BFIN && !ADK_TARGET_ARCH_C6X && !ADK_TARGET_ARCH_FRV && !ADK_TARGET_ARCH_H8300 && !ADK_TARGET_ARCH_LM32
